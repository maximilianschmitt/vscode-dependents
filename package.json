{
	"name": "dependents",
	"displayName": "dependents",
	"publisher": "maximilianschmitt",
	"description": "Find dependents of a JS file",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/maximilianschmitt/vscode-dependents"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.findDependents"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.findDependents",
				"title": "Find dependents"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"dependents": "^3.3.1"
	}
}
